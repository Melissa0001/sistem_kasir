x = int(input("masukan jumlah boneka: "))
if x <=12:
    print("harga per boneka Rp 20000 ")
    print("total",x*20000)
elif x <=24:
    print("harga per boneka Rp 19500")
    print("total",x*19500)
elif x <=50:
    print("harga per boneka Rp 18000")
    print("total",x*18000)
else:
    print("harga per boneka Rp 17000")
    print("total",x*17000
